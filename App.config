<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="SINGLESALE" value="^(\w+)\s+at\s+(\d+(\.\d+)?)[pP]$"/>
		<add key="MULTIPLESALE" value="^(\d+) sales of (\w+) at (\d+(\.\d+)?)p each$"/>
		<add key="ADJUSTMENT" value="^(\w+) (\d+(\.\d+)?)p (\w+)$"/>
	</appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>